<%= form_tag user_page_list_goal_path(current_user.id,params[:page_id],params[:list_id],@goal.id), method: 'put', :class => "form-inline form-group", id: "goal_update_#{@goal.id}", remote: true do %>
    <%= text_field_tag "goal[name]", @goal.name, class: "form-control" %>
    <%= submit_tag 'update goal', class: "btn btn-primary" %>
  <% end %>