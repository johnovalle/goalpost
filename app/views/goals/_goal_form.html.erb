<%= form_tag user_page_list_goals_path(current_user.id,params[:page_id],params[:list_id]), :class => "form-inline", id: "goal_form_#{params[:list_id]}", remote: true do %>
    <%= text_field_tag "goal[name]" %>
    <%= hidden_field_tag "goal[list_id]", params[:list_id] %>
    <%= submit_tag "make a new goal" %>
  <% end %>