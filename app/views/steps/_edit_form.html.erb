<%= form_tag user_page_list_goal_step_path(current_user.id,params[:page_id],params[:list_id],params[:goal_id],@step.id), method: 'put', :class => "form-inline form-group", id: "step_update_#{@step.id}", remote: true do %>
  <%= text_field_tag "step[name]", @step.name, class: "form-control" %>
  <%= submit_tag 'update step name', class: "btn btn-primary" %>
<% end %>
