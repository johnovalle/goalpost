<%= form_tag user_page_list_path(current_user.id,params[:page_id],@list.id), method: 'put', :class => "form-inline form-group", id: "list_update_#{@list.id}", remote: true do %>
  <%= text_field_tag "list[name]", @list.name, class: "form-control" %>
  <%= submit_tag 'update list', class: "btn btn-primary" %>
<% end %>