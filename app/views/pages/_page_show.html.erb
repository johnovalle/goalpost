<h1><%= link_to page.name, edit_user_page_path(current_user.id,page.id), id: "page_name_#{page.id}", class:"page_name", remote: true %></h1>
<%= link_to 'Add a list', new_user_page_list_path(current_user.id,page.id), id: "new_page_link_#{page.id}", remote: true %>
<br/>
<br/>
<section id="page_<%= page.id %>">
  <% page.lists.each do |list| %>
    <%= render :partial => "lists/list", :locals => {:page => page, :list => list} %>
  <% end %>
  </section>   